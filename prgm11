#include <stdio.h>
int main() 
{   
    int a,b;
    printf("Enter first integer: ");
    scanf("%d",&a);
    printf("\nEnter second integer: ");
    scanf("%d",&b);
    if (a == b) 
    {
        printf("\nThe values are equal");
    } else 
    {
        printf("\nThe values are not equal");
    }

      
    return 0;
}
